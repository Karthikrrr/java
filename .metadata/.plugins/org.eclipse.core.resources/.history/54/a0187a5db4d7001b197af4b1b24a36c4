package ADT;

class Node
{
	int data;
	Node next;
	Node(int data)
	{
		this.data=data;
		next=null;
	}
}

public class LinkedList {
	Node head;
	
	static class Node
	{
		int data;
		Node next;
		Node(int data)
		{
			this.data=data;
			next=null;
		}
	}
	
	LinkedList()
	{
		head=null;
	}
//	
//	Node reverse(Node node)
//	{
//		Node prev=null;
//		Node curr=node;
//		Node next=null;
//		while(curr!=null)
//		{
//			
//		}
//		
//	}
//	
	
	
	void printy(Node node)
	{
		while(node!=null)
		{
			System.out.print(node.data + " ");
			node=node.next;
		}
	}
	
	public static void main(String[] args)
	{
		LinkedList l= new LinkedList();
		l.head=new Node(10);
		l.head.next=new Node(20);
		l.head.next.next= new Node(30);
		l.printy(head);
	}

}
