package Link;



public class Reversell
{

static Node head;	

static class Node
{
int data;
Node next;

Node(int data)
{
this.data=data;
next=null;
}
}


static void display(Node node)
{
Node temp=head;
while(temp.next!=null)
{
System.out.print(temp.data);
temp=temp.next;
}
}



public void main(String[] args)
{
head=null;
head=new Node(1);
head.next=new Node(2);
head.next.next=new Node(3);
}
}
