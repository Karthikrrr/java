package jo;

public class SecMAx 
{
	public void check(int[] arr)
	{
		int n=arr.length;
		int max=0;
		for(int i=0;i<n;i++)
		{
			if(arr[i]>max)
			{
				max=arr[i];
		
			}
		}
		System.out.print(max);
	}
	
public static void main(String[] args)
{
int[] arr=new int[] {1,12,123,652};
SecMAx s=new SecMAx();
s.check(arr);
}
}
